#include <stdio.h>
#include <stdlib.h>

int		ft_atoi(char *str);

int main(int argc, char **argv)
{
	argc = 4;
	int r;
	int i;

	i = 1;
	r = ft_atoi(argv[i]) + ft_atoi(argv[i + 2]);
	
	printf("%d", r);
	return(0);
}
